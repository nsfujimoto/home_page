<!DOCTYPE html>
<html>
  <head>
    <title><%= @page_title + " - " if @page_title.present?%>管理ページ</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

		<link rel="stylesheet" href="/highlight/styles/tomorrow-night.css" data-turbolinks-track="reload">
		<script src="/highlight/highlight.pack.js" data-turbolinks-track="reload"></script>


		<!-- Google Analytics -->
		<!-- Global Site Tag (gtag.js) - Google Analytics -->
		<script async src="https://www.googletagmanager.com/gtag/js?id=UA-106472668-1"></script>
		<script>
		  window.dataLayer = window.dataLayer || [];
 			function gtag(){dataLayer.push(arguments)};
  		gtag('js', new Date());

  		gtag('config', 'UA-106472668-1');
		</script>
		<!-- Google Analytics -->

  </head>

  <body id="body" style="min-width: 900px" >
		<!-- Header -->
		<div id="manage_header">
			<%= render "shared/user_header" %>
		</div>
		<!-- Header -->

		<!-- Content -->
		<div id="manage_wrapper">
		
		<!-- Sidebar -->
		<div id="sidebar_container">
		<nav id="manage_sidebar">
			<%= render "shared/manage_sidebar" %>
		</nav>
		</div>
		<!-- Sidebar -->
		
		<!-- MainContent -->
		<div id="manage_container">
			<div id="main_content">
			<%= yield %>
			</div>
		</div>
		<!-- MainContent -->

		</div>
		<!-- Content -->

		<!-- Footer -->
		<div id="manage_footer">
		<%= render "shared/manage_footer" %>
		</div>
		<!-- Footer -->
		<script>hljs.initHighlighting();</script>
  </body>
</html>
